{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "projects/mat-image-grid-lib/src/index.ts",
    "projects/full-demo/src/main.ts",
    "**/*.spec.ts",
    "projects/mat-image-grid-lib/karma.conf.js",
    "projects/full-demo/cypress.config.ts",
    "projects/full-demo/cypress/e2e/*.ts",
    "projects/full-demo/karma.conf.js",
    "projects/minimal-demo/cypress.config.ts",
    "projects/minimal-demo/cypress/e2e/*.ts",
    "projects/minimal-demo/karma.conf.js",
    "tools/npm-scripts/readme-updater.js",
    "tools/create-mockup-extended-grid.mjs"
  ],
  "ignore": [
    "projects/full-demo/cypress/support/*.ts",
    "projects/minimal-demo/cypress/support/*.ts"
  ],
  "ignoreDependencies": [
    "@angular/compiler",
    "@angular-eslint/schematics",
    "@angular-eslint/template-parser",
    "@angular/platform-browser-dynamic",
    "@cypress/schematic",
    "@fontsource/material-icons",
    "@fontsource/roboto",
    "@types/jasmine",
    "eslint-plugin-cypress"
  ],
  "ignoreBinaries": ["knip", "check-is-deprecated"]
}
